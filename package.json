{
  "name": "aof-client-builder",
  "version": "0.0.1",
  "description": "Open source cross platform client to record, find and watch League of Legends replays",
  "scripts": {
    "postinstall": "npm install --prefix app",
    "start": "npm run dev",
    "dev": "concurrently --kill-others \"npm run dev-server\" \"npm run dev-client\"",
    "dev-client": "cross-env NODE_ENV=development electron -r babel-register app/",
    "dev-server": "cross-env NODE_ENV=development node -r babel-register server.js",
    "test": "./node_modules/.bin/mocha test/test.js",
    "test-e2e": "./node_modules/.bin/mocha test/e2e.js",
    "dist": "./node_modules/.bin/build --platform darwin --platform win32 --arch x64"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AnalyzeOrFeed/aof-client.git"
  },
  "keywords": [
    "Replay",
    "LoL",
    "League Of Legends Replay",
    "League Of Legends",
    "LeagueOfLegends",
    "Analyze Or Feed",
    "AnalyzeOrFeed",
    "aof.gg",
    "aof"
  ],
  "contributors": [
    {
      "name": "Andreas Gassmann",
      "email": "andy@aof.gg"
    },
    {
      "name": "Marco Crespi",
      "email": "marco@aof.gg"
    }
  ],
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.7.2",
    "concurrently": "^2.0.0",
    "copy-webpack-plugin": "^2.1.3",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "electron-builder": "^3.16.1",
    "electron-prebuilt": "^0.37.8",
    "express": "^4.13.4",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
    "mocha": "^2.4.5",
    "node-sass": "^3.6.0",
    "react": "^15.0.2",
    "react-transform-hmr": "^1.0.4",
    "sass-loader": "^3.2.0",
    "spectron": "^2.37.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "build": {
    "app-bundle-id": "gg.aof.client",
    "app-category-type": "public.app-category.entertainment",
    "iconUrl": "https://raw.githubusercontent.com/AnalyzeOrFeed/aof-replay-client/master/src/app/icons/aof.ico",
    "osx": {},
    "win": {}
  }
}
