{
  "name": "aof-client-builder",
  "version": "0.0.1",
  "description": "Open source cross platform client to record, find and watch League of Legends replays",
  "scripts": {
    "postinstall": "cd app && npm install",
    "start": "npm run dev",
    "dev": "concurrently --kill-others \"npm run dev-server\" \"npm run dev-client\"",
    "dev-client": "cross-env NODE_ENV=development electron -r babel-register app/",
    "dev-server": "cross-env NODE_ENV=development node -r babel-register server.js",
    "test": "mocha test/test.js",
    "test-e2e": "mocha test/e2e.js",
    "dist": "node -r babel-register build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AnalyzeOrFeed/aof-client.git"
  },
  "keywords": [
    "Replay",
    "LoL",
    "League Of Legends Replay",
    "League Of Legends",
    "LeagueOfLegends",
    "Analyze Or Feed",
    "AnalyzeOrFeed",
    "aof.gg",
    "aof"
  ],
  "contributors": [
    {
      "name": "Andreas Gassmann",
      "email": "andy@aof.gg"
    },
    {
      "name": "Marco Crespi",
      "email": "marco@aof.gg"
    }
  ],
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-preset-electron": "^0.37.8",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.11.6",
    "concurrently": "^2.2.0",
    "copy-webpack-plugin": "^3.0.1",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "electron-builder": "^5.25.1",
    "electron-prebuilt": "^1.3.3",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "mocha": "^3.0.2",
    "node-sass": "^3.8.0",
    "react": "^15.3.0",
    "react-transform-hmr": "^1.0.4",
    "sass-loader": "^4.0.0",
    "spectron": "^3.3.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.20",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2"
  },
  "build": {
    "app-bundle-id": "gg.aof.client",
    "app-category-type": "public.app-category.entertainment",
    "iconUrl": "https://raw.githubusercontent.com/AnalyzeOrFeed/aof-replay-client/master/src/app/icons/aof.ico",
    "mac": {},
    "win": {}
  },
  "directories": {
    "app": "./temp"
  }
}
